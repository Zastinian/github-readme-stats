{
  "name": "stats-for-github-readme",
  "version": "1.0.0",
  "description": "GitHub's Stats in SVG",
  "main": "src/server.ts",
  "scripts": {
    "dev": "bun run --watch src/server.ts",
    "build": "tsc",
    "build:linux": "bun build --compile --target=bun-linux-x64 src/server.ts --outfile stats",
    "build:windows": "bun build --compile --target=bun-windows-x64 src/server.ts --outfile stats",
    "start": "bun ./dist/server.js",
    "start:linux": "./stats",
    "start:windows": "./stats.exe"
  },
  "author": "contact@zastinian.com",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zastinian/stats-for-github-readme.git"
  },
  "keywords": [
    "github-readme-stats",
    "github-stats",
    "readme-stats",
    "readme",
    "github",
    "stats",
    "stats-for-github-readme"
  ],
  "bugs": {
    "url": "https://github.com/Zastinian/stats-for-github-readme/issues"
  },
  "homepage": "https://github.com/Zastinian/stats-for-github-readme#readme",
  "dependencies": {
    "@elysiajs/html": "^1.0.2",
    "axios": "^1.6.8",
    "elysia": "^1.0.20",
    "millify": "^6.1.0",
    "node-base64-image": "^2.0.6"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typescript": "^5.4.5"
  }
}
